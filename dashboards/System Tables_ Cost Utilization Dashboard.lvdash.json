      "name": "14087b30",
      "displayName": "shared cluster cost",
      "query": "SELECT cluster_name,date_trunc('MONTH',usage_date) usage_month, cluster_data_security_mode, sum(day_cluster_est_dbu_cost) est_dbu_cost , 2*est_dbu_cost as est_tot_cost \nFROM finops.system_lookups_dims.v_shared_cluster_job_duration_cost\n WHERE (CASE WHEN :workspace_name = 'all' THEN 1=1 ELSE ARRAY_CONTAINS(split(lower(:workspace_name),\";\"),lower(workspace_name)) END)\n AND (CASE WHEN :cluster_name = 'all' THEN 1=1 ELSE startswith(lower(cluster_name),lower(:cluster_name)) END)\nAND (CASE WHEN :all_time = 'all'  THEN 1=1 ELSE date_trunc('MONTH',usage_date) = date_trunc('MONTH',:date_of_month) END)\n GROUP BY cluster_name , date_trunc('MONTH',usage_date), cluster_data_security_mode\n ORDER BY  cluster_name ASC",

{
  "datasets": [
    {
      "name": "14087b30",
      "displayName": "shared cluster cost",
      "query": "SELECT cluster_name,MONTH(usage_date), cluster_access_mode, sum(day_cluster_est_dbu_cost) est_dbu_cost , 2*est_dbu_cost as est_tot_cost \nFROM akrinsky_dbsql_logging.finops.v_shared_cluster_job_duration_cost\n WHERE (CASE WHEN :workspace_name = 'all' THEN 1=1 ELSE ARRAY_CONTAINS(split(lower(:workspace_name),\";\"),lower(workspace_name)) END)\n AND (CASE WHEN :cluster_name = 'all' THEN 1=1 ELSE startswith(lower(cluster_name),lower(:cluster_name)) END)\n AND MONTH(usage_date) = MONTH(:date_of_month)\n GROUP BY cluster_name , MONTH(usage_date), cluster_access_mode\n ORDER BY  cluster_name ASC",
      "parameters": [
        {
          "displayName": "workspace_name",
